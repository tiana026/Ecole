        <th>
            @Html.DisplayNameFor(model => model.Annee.AnneeNom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Promotion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Annee.AnneeNom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Promotion)
        </td>
